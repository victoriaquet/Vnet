

@{
    ViewBag.Title = "SubirArchivo";
}

<h2>SubirArchivo</h2>

@using (Html.BeginForm("TratarArchivo", "VnetRegistro", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="container">
        <div class="panel panel-primary ">

            <div class="panel-heading">
                Busca y añade el archivo V-net más reciente
            </div>

            <div class="form-group">

                <input class="form-control-file" type="file" name="file" />

                <input class="btn btn-success" type="submit" />

            </div>

            <br />
            <b>
                @ViewBag.mensaje
            </b>

        </div>

    </div>
}
