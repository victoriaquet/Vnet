                        {
                            <option value="@item.IdOferta#@item.CantTarjetasAdicionales#@item.CantDias#@item.Precio">@item.Nombre + @item.CantDias</option>

                        }
                    </select>
                    @Html.ValidationMessageFor(model => model.IdOferta, "", new { @class = "text-danger" })
                </div>
                <input class="hidden" value="" id="IdOferta" required name="IdOferta" />
                <input class="hidden" value="" id="cdias" />


            </div>
            <div class="form-group" style="display: flex">
                <label class="control-label " for="CantTarjetasAdicionales" style="font-size: inherit; width: 40%">Tarjetas adic.</label>
                <input class="form-control" id="CantTarjetasAdicionales" readonly="readonly" name="CantTarjetasAdicionales" style="width: auto; font-weight: bold">

            </div>

            <div class="form-group divsus">
                @Html.LabelFor(model => model.CantTarjetasExtras, htmlAttributes: new { @class = "control-label " })

                <input class="form-control" type="number" id="CantTarjetasExtras" value="0" min="0" name="CantTarjetasExtras" style="width: auto; font-weight: bold">
                @Html.ValidationMessageFor(model => model.CantTarjetasExtras, "", new { @class = "text-danger" })

            </div>
           

        </div>



        <div class="col-lg-6" style="display: grid">
            <div class="form-group divsus precioreferenciadiv">
                @Html.Label("Precio (*)", htmlAttributes: new { @class = "control-label precioreferencialabel" })

                @Html.EditorFor(model => model.PrecioSuscripcion, new { htmlAttributes = new { @class = "form-control precioreferenciainput" } })
                @Html.ValidationMessageFor(model => model.PrecioSuscripcion, "", new { @class = "text-danger" })

            </div>
            <div style="display: flex">
                <div class="form-group margen-top">
                    @Html.LabelFor(model => model.Lunes, htmlAttributes: new {@class = "control-label labelcheckboxdias"})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Lunes, new {@class = "diasEdit"})
                    </div>
                </div>
                <div class="form-group margen-top" style="margin-left: 20px">
                    @Html.LabelFor(model => model.Martes, htmlAttributes: new {@class = "control-label labelcheckboxdias "})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Martes, new {@class = "diasEdit"})
                    </div>
                </div>
                <div class="form-group margen-top" style="margin-left: 20px">
