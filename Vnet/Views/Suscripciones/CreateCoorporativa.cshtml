                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Miercoles, new {@class = "diasEdit"})
                    </div>
                </div>



                <div class="form-group margen-top" style="margin-left: 20px">
                    @Html.LabelFor(model => model.Jueves, htmlAttributes: new {@class = "control-label labelcheckboxdias"})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Jueves, new {@class = "diasEdit"})
                    </div>
                </div>



                <div class="form-group margen-top" style="margin-left: 20px">
                    @Html.LabelFor(model => model.Viernes, htmlAttributes: new {@class = "control-label labelcheckboxdias"})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Viernes, new {@class = "diasEdit"})
                    </div>
                </div>



                <div class="form-group margen-top" style="margin-left: 20px">
                    @Html.LabelFor(model => model.Sabado, htmlAttributes: new {@class = "control-label labelcheckboxdias"})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Sabado, new {@Class = "diasEdit"})
                    </div>
                </div>



                <div class="form-group margen-top" style="margin-left: 20px">
                    @Html.LabelFor(model => model.Domingo, htmlAttributes: new {@class = "control-label labelcheckboxdias"})
                    <div class="text-center">
                        @Html.CheckBoxFor(model => model.Domingo, new {@class = "", @disabled = "disabled", @checked = "checked"})
                    </div>
                </div>
            </div>
            <div><span class="text-danger customValidation" id="errordias"></span>
            </div>
        </div>
    </section>
