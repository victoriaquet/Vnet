                <div>
                    <select class="form-control" id="Editorial" name="Editorial" style="width: auto; font-weight: bold">

                        <option value="1">Chaco</option>
                        <option value="2">Corrientes</option>

                    </select>

                </div>
            </div>
            <div id="ofertasChaco" style="display: none">
                <option value="">Seleccionar Oferta...</option>
                @foreach (var item in @ViewBag.ofertasChaco)
                {
                    <option value="@item.IdOferta#@item.CantTarjetasAdicionales#@item.CantDias#@item.Precio">@item.Nombre + @item.CantDias</option>

                }
            </div>

            <div id="ofertasCorrientes" style="display: none">
                <option value="">Seleccionar Oferta...</option>
                @foreach (var item in @ViewBag.ofertasCtes)
                {
                    <option value="@item.IdOferta#@item.CantTarjetasAdicionales#@item.CantDias#@item.Precio">@item.Nombre + @item.CantDias</option>

                }
            </div>


            <div class="form-group" style="display: flex">

                @Html.Label("Oferta (*)", htmlAttributes: new { @class = "control-label ", @style = "font-size:inherit;width: 20%" })

