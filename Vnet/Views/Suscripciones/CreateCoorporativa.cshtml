@section Scripts{


    <script src="~/Scripts/localidadesPorProv.js"></script>
    <script src="~/Scripts/Suscripciones/createcoorporativa.js"></script>


    <script>

        $(document).ready(function() {
            $("#linesuscriptor").change(function() {
                var sdata = $('#linesuscriptor').val();
                //DATOS PERSONALES

                var IdSuscriptor = sdata.split("#")[0];
                var NumeroSuscriptor = sdata.split("#")[1];
                var Nombre = sdata.split("#")[2];
                var Apellido = sdata.split("#")[3];
                var FechaNacimiento = sdata.split("#")[4];
                var DNI = sdata.split("#")[5];
                var TipoSexo = sdata.split("#")[6];
                if (TipoSexo == "Masculino") {
                    TipoSexo = 1
                };
                if (TipoSexo == "Femenino") {
                    TipoSexo = 2
                };
                if (TipoSexo == "Otro") {
                    TipoSexo = 3
                };
                var Email = sdata.split("#")[7];
                var CUIT = sdata.split("#")[8];
                var RazonSocial = sdata.split("#")[9];
                var TelefonoFijoArea = sdata.split("#")[10];
                var TelefonoFijoNumero = sdata.split("#")[11];
                var TelefonoMovilArea = sdata.split("#")[12];
                var TelefonoMovilNumero = sdata.split("#")[13];
                //DATOS DOMICILIO

                var IdDomicilio = sdata.split("#")[14];
                var Calle = sdata.split("#")[15];
                var Altura = sdata.split("#")[16];
                var Piso = sdata.split("#")[17];
                var Departamento = sdata.split("#")[18];
                var CalleLateral1 = sdata.split("#")[19];
                var CalleLateral2 = sdata.split("#")[20];
                var Barrio = sdata.split("#")[21];
                var IdLocalidad = sdata.split("#")[22];
                var IdProvincia = sdata.split("#")[23];
                var Observaciones = sdata.split("#")[24];

                $("#IdSuscriptor").val(IdSuscriptor);
                $("#NumeroSuscriptor").val(NumeroSuscriptor);
                $("#Nombre").val(Nombre);
                $("#NombresTarjetaClub").val(Nombre);
                $("#Apellido").val(Apellido);
                $("#ApellidoTarjetaClub").val(Apellido);
                $("#FechaNacimiento").val(FechaNacimiento);
                $("#DNI").val(DNI);
                $("#TipoSexo").val(TipoSexo);
                $("#Email").val(Email);
                $("#CUIT").val(CUIT);
                $("#RazonSocial").val(RazonSocial);
                $("#TelefonoFijoArea").val(TelefonoFijoArea);
                $("#TelefonoFijoNumero").val(TelefonoFijoNumero);
                $("#TelefonoMovilArea").val(TelefonoMovilArea);
                $("#TelefonoMovilNumero").val(TelefonoMovilNumero);

                $("#IdDomicilio").val(IdDomicilio);
                $("#CalleSuscriptor").val(Calle);
                $("#AlturaSuscriptor").val(Altura);
                $("#PisoSuscriptor").val(Piso);
                $("#DepartamentoSuscriptor").val(Departamento);
                $("#CalleLateral1Suscriptor").val(CalleLateral1);
                $("#CalleLateral2Suscriptor").val(CalleLateral2);
                $("#BarrioSuscriptor").val(Barrio);
                $("#IdProvincia").val(IdProvincia).trigger("change");
                $("#linelocalidad").val(IdLocalidad);

                $("#NombreFacturacion").val(Nombre);
                $("#ApellidoFacturacion").val(Apellido);
                $("#DNIFacturacion").val(DNI);
                $("#CUITFacturacion").val(CUIT);
                $("#RazonSocialFacturacion").val(RazonSocial);

                $("#CalleFacturacion").val(Calle);
                $("#AlturaFacturacion").val(Altura);
                $("#PisoFacturacion").val(Piso);
                $("#DepartamentoFacturacion").val(Departamento);
                $("#CalleLateral1Facturacion").val(CalleLateral1);
                $("#CalleLateral2Facturacion").val(CalleLateral2);
                $("#BarrioFacturacion").val(Barrio);
                $("#IdProvinciaFacturacion").val(IdProvincia).trigger("change");
                $("#linelocalidadfacturacion").val(IdLocalidad);
                $("#ObservacionesDomicilioFacturacion").val(Observaciones);
                $(".divsus").removeClass("is-empty");


            });

          

            $(".js-example-basic-single").select2({
                disabled: true
            });
            $(".selectsuscriptoressecundarios").select2({
                placeholder: 'Agregar tarjeta club...',
                width: 'resolve',
                disabled: true
            });
            //Carga los datos del suscriptor precargado
            var msg = @Html.Raw(Json.Encode(ViewBag.idsus));
            if (msg != null & msg != "") {
                $("#linesuscriptor").trigger('change');
            };
            //


        });
    
    </script>
}



